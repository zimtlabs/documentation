{
  "name": "documentation",
  "version": "1.3.9",
  "private": true,
  "scripts": {
    "start:dev": "cross-env NODE_ENV=development nodemon ./src",
    "start:server": "cross-env NODE_ENV=production node ./src",
    "start": "npm run start:server",
    "meta": "node ./scripts/generate-meta",
    "prebuild": "npm run meta",
    "build": "next build",
    "export": "npm run build && next export",
    "a": "git pull && git merge origin/master && npm version patch --force && npm run build && git add ."
  },
  "nodemonConfig": {
    "watch": [
      "src",
      "server"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@material-ui/styles": "^4.11.3",
    "@sentry/browser": "^5.30.0",
    "@zimt/sdk": "^1.15.1",
    "axios": ">=0.21.1",
    "clsx": "^1.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "helmet": "^3.23.3",
    "js-cookie": "^2.2.1",
    "marked": "^0.8.2",
    "method-override": "^3.0.0",
    "mobx": "^5.15.4",
    "next": "^10.0.9",
    "next-seo": "^4.20.0",
    "nextjs-progressbar": "0.0.7",
    "prismjs": "^1.24.0",
    "react": "^16.14.0",
    "react-app-polyfill": "^1.0.6",
    "react-dom": "^16.14.0",
    "react-ga": "^2.7.0",
    "redoc": "2.0.0-rc.41",
    "rxjs": "^6.6.6",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@svgr/webpack": "^5.4.0",
    "eslint": "^6.8.0",
    "nodemon": "^2.0.3",
    "object-path": ">=0.11.5",
    "node-fetch": ">=2.6.1",
    "elliptic": ">=6.5.4"
  }
}
