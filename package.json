{
  "name": "documentation",
  "version": "1.1.28",
  "private": true,
  "scripts": {
    "start:dev": "cross-env NODE_ENV=development nodemon ./src",
    "start:server": "cross-env NODE_ENV=production node ./src",
    "start": "npm run start:server",
    "meta": "node ./scripts/generate-meta",
    "prebuild": "npm run meta",
    "build": "next build",
    "export": "npm run build && next export",
    "a": "git pull && git merge origin/master && npm version patch --force && npm run build && git add ."
  },
  "nodemonConfig": {
    "watch": [
      "src",
      "server"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@material-ui/styles": "^4.10.0",
    "@sentry/browser": "^5.22.0",
    "@zimt/sdk": "^1.14.8",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "clsx": "^1.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "express": "^4.17.1",
    "helmet": "^3.22.0",
    "marked": "^0.8.0",
    "method-override": "^3.0.0",
    "mobx": "^5.15.4",
    "next": "^9.4.4",
    "next-seo": "^4.5.0",
    "nextjs-progressbar": "0.0.7",
    "prismjs": "^1.22.0",
    "react": "^16.14.0",
    "react-app-polyfill": "^1.0.6",
    "react-dom": "^16.8.0",
    "react-ga": "^2.7.0",
    "redoc": "2.0.0-rc.41",
    "rxjs": "^6.5.4",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@svgr/webpack": "^5.4.0",
    "eslint": "^6.8.0",
    "nodemon": "^2.0.3"
  }
}
